DTC_FLAGS+="-@"

if ([[ $BSP_SPECIFIED_KO_CONFIG == "true" ]] || [[ $BSP_KOONLY_CONFIG == "true" ]]) && ([[ $BSP_UD_PREBUILTS_FLAG == "true" ]]); then
	MAKE_GOALS="$BSP_MAKE_GOALS"
