KERNEL_DIR=${KERNEL_DIR}"/.."
. ${ROOT_DIR}/${KERNEL_DIR}/build.config.gki.aarch64

BUILD_SYSTEM_DLKM=0
DEFCONFIG=ums312_gki_defconfig
FRAGMENT_CONFIG=${KERNEL_DIR}/arch/arm64/configs/sprd_gki_sharkl5_debug.fragment
UMS312_BASE_CONFIG="${ROOT_DIR}/${KERNEL_DIR}/arch/arm64/configs/gki_defconfig
                    ${ROOT_DIR}/${KERNEL_DIR}/arch/arm64/configs/sprd_gki_sharkl5.fragment
                   "

if [[ ${BSP_PRODUCT_NAME} == *Skynet* ]]; then
    UMS312_BASE_CONFIG+="${ROOT_DIR}/${KERNEL_DIR}/arch/arm64/configs/sprd_swkasan_Skynet.fragment"
fi

PRE_DEFCONFIG_CMDS="KCONFIG_CONFIG=${ROOT_DIR}/${KERNEL_DIR}/arch/arm64/configs/${DEFCONFIG} \
${ROOT_DIR}/${KERNEL_DIR}/scripts/kconfig/merge_config.sh -m -r ${UMS312_BASE_CONFIG} ${ROOT_DIR}/${FRAGMENT_CONFIG}"
POST_DEFCONFIG_CMDS="rm ${ROOT_DIR}/${KERNEL_DIR}/arch/arm64/configs/${DEFCONFIG}"

if [[ $BSP_SPECIFIED_KO_CONFIG == "true" && $BSP_UD_PREBUILTS_FLAG == "true" ]]; then
	EXT_MODULES="$BSP_EXT_MODULES"
elif [[ $BSP_DTSONLY_CONFIG == "true" ]]; then
	SKIP_EXT_MODULES=1
else
EXT_MODULES="
../modules/kernel5.15/sample
../modules/kernel5.15/display/gsp
