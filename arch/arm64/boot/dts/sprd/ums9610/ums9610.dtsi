#include "ddr-dfs.dtsi"
#include "dispc.dtsi"
#include "dma.dtsi"
#include "dmc-mpu.dtsi"
#include "efuse.dtsi"
#include "glbreg.dtsi"
#include "gpio.dtsi"
#include "gpu-natt.dtsi"
#include "i2c.dtsi"
#include "i2s.dtsi"
#include "jpg.dtsi"
#include "modem.dtsi"
#include "pin-controller.dtsi"
#include "power.dtsi"
#include "sensorhub.dtsi"
#include "spi.dtsi"
#include "thermal.dtsi"
#include "uart.dtsi"
