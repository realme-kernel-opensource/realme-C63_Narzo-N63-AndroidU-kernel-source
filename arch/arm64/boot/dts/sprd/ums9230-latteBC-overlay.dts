 		 function = "func1";

 	};

 	jiiov_fp_gpio_miso:SPI_MISO {
 		 pins = "QOGIRL6_SPI0_DI";
 		 function = "func1";
 	};

 	jiiov_fp_gpio_mosi:SPI_MOSI {
