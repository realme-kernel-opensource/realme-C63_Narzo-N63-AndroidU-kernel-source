&pmic_fgu {
	status = "ok";
	//charger = <&sgm41542>;
	FUELGAGUE_APPLY = <(1)>;
	BATT_ID_FAST_CHECK = <(1)>;
	Enable_Is_Force_Full = <(1)>;
	sprd,calib-resistance-micro-ohms = <5000>;
	sprd,comp-resistance-mohm = <4>;
