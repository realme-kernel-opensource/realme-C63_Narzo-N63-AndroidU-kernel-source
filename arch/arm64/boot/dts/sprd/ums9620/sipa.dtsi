/ {
	aliases {
		eth0 = &sipa_eth0;
		eth1 = &sipa_eth1;
		eth2 = &sipa_eth2;
		eth3 = &sipa_eth3;
		eth4 = &sipa_eth4;
		eth5 = &sipa_eth5;
		eth6 = &sipa_eth6;
		eth7 = &sipa_eth7;
		eth8 = &sipa_eth8;
		eth9 = &sipa_eth9;
		eth10 = &sipa_eth10;
		eth11 = &sipa_eth11;
		eth12 = &sipa_eth12;
		eth13 = &sipa_eth13;
		eth14 = &sipa_eth14;
		eth15 = &sipa_eth15;
    };

	sipa_eth0: sipa-eth0 {
		compatible = "sprd,sipa_eth";
		sprd,netid = <1>;
		sprd,term-type = <0x6>;
