#include "i2c.dtsi"
#include "i2s.dtsi"
#include "jpg.dtsi"
#include "mipi-serdes.dtsi"
#include "modem.dtsi"
#include "npu.dtsi"
#include "pin-controller.dtsi"
#include "audio.dtsi"
#include "power.dtsi"
#include "ptm.dtsi"
#include "sensorhub.dtsi"
#include "spi.dtsi"
#include "thermal.dtsi"
#include "timer.dtsi"
